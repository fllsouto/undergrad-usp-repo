#==================================================================================================================================================

CFLAGS = -g -I. -Wall -pedantic -ansi
LDFLAGS = -L.
LDLIBS = -lm
RM = /bin/rm -f
CC = gcc
comunObj=client.o interface.o readBook.o fundamentalHash.o
wordObj=handlingWord.o
lemmaObj= handlingLemma.o
HashingEnc2=wordEnc.o lemmaEnc.o
HashingEncLp=wordEnc.o lemmaProb.o
HashingLpEnc=wordProb.o lemmaEnc.o
HashingLp2=wordProb.o lemmaProb.o

#==================================================================================================================================================

#>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>

clean:
	$(RM)  *.o

#<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<


#>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>


cleanexec:
	$(RM) ep4*

#<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<


#>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>

#Estrutura Word = Hashing Encadeado
#Estrutura Lema = Hashing Encadeado

ep4encenc: $(comunObj) $(lemmaObj) $(wordObj) $(HashingEnc2)  
	$(CC) $(CFLAGS) -o ep4encenc $(comunObj) $(lemmaObj) $(wordObj) $(HashingEnc2)  $(LDLIBS)

#<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<


#>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>

#Estrutura Word = Hashing Probing Linear
#Estrutura Lema = Hashing Probing Linear

ep4lplp: $(comunObj) $(lemmaObj) $(wordObj) $(HashingLp2)  
	$(CC) $(CFLAGS) -o ep4lplp $(comunObj) $(lemmaObj) $(wordObj) $(HashingLp2)  $(LDLIBS)
	
#<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<


#>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>

#Estrutura Word = Hashing Probing Linear
#Estrutura Lema = Hashing Encadeado

ep4lpenc: $(comunObj) $(lemmaObj) $(wordObj) $(HashingLpEnc)  
	$(CC) $(CFLAGS) -o ep4lpenc $(comunObj) $(lemmaObj) $(wordObj) $(HashingLpEnc)  $(LDLIBS)

#<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<

	
#>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>

#Estrutura Word = Hashing Encadeado
#Estrutura Lema = Hashing Probing Linear

ep4enclp: $(comunObj) $(lemmaObj) $(wordObj) $(HashingEncLp)  
	$(CC) $(CFLAGS) -o ep4enclp $(comunObj) $(lemmaObj) $(wordObj) $(HashingEncLp)  $(LDLIBS)
	
#<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<


###################################################################################################################################################


#**************************************************************************************************************************************************
$(comunObj): readBook.h interface.h

#----------------------------------------------------------------------------------------------------

interface.o: handlingLemma.h lemmaEnc.h lemmaProb.h   handlingWord.h wordEnc.h wordProb.h 

#----------------------------------------------------------------------------------------------------

$(lemmaObj): lemmaEnc.h lemmaProb.h lemmaDefs.h

$(wordObj): wordEnc.h wordProb.h wordDefs.h

$(HashingEnc2): fundamentalHash.h lemmaDefs.h wordDefs.h

$(HashingLp2): fundamentalHash.h lemmaDefs.h wordDefs.h


#**************************************************************************************************************************************************
